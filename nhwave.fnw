     if(n_east.eq.MPI_PROC_NULL) then
# endif
       do j = Jbeg,Jend
       do k = Kbeg,Kend
       do i = 1,Nghost
         phi(Iend+i,j,k) = phi(Iend-i+1,j,k)
       enddo
       enddo
       enddo
# if defined (PARALLEL)
     endif
# endif

     ! y-direction and corners
# if defined (PARALLEL)
     if(n_suth.eq.MPI_PROC_NULL) then
# endif
       do i = 1,Mloc
       do k = Kbeg,Kend
       do j = 1,Nghost
         phi(i,j,k) = phi(i,Jbeg+Nghost-j,k)
       enddo
       enddo
       enddo
# if defined (PARALLEL)
     endif
# endif

# if defined (PARALLEL)
     if(n_nrth.eq.MPI_PROC_NULL) then
# endif
       do i = 1,Mloc
       do k = Kbeg,Kend
       do j = 1,Nghost
         phi(i,Jend+j,k) = phi(i,Jend-j+1,k)
       enddo
       enddo
       enddo
